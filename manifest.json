{
    "manifest_version": 3,
    "name": "Page Text Extractor",
    "version": "1.0",
    "description": "Extracts all data from the current page and downloads it.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ]
  }
  